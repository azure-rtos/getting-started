    "version": "0.2.0",
    "configurations": [
          {
            "name": "OpenOCD Debug L475E",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "interface": "swd",
            "device": "STM32L4x",
            "cwd": "${workspaceRoot}",
            "rtos": "auto",
            "executable": "./build/app/stm32l475_azure_iot.elf",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32l4x.cfg"
            ],
            "openOCDLaunchCommands": [
                "telnet_port 1336"
            ],
            "svdFile": "${workspaceRoot}/.vscode/STM32L4x5.svd",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 80000000,
                "swoFrequency": 2000000,
                "source": "probe"
            },
        }
      ]
  }